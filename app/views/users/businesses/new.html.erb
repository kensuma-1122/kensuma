<% provide(:title, "事業所登録") %>
<% provide(:btn_text, "登録") %>

<div class="card">
  <div class="card-body">
    <%= form_with model: @business_new, url: users_business_path, method: :post, local: true do |f| %>

      <%= render partial: "form", locals: { f: f, business: @business_new } %>

      <%= f.submit yield(:btn_text), class: "btn btn-md btn-block btn-primary" %>
    <% end %>
  </div>
</div>
