<% provide(:title,  "DashBoard") %>
<div class="content">
  <div class="container-fluid">
    <div class="row">
    
      <div class="col-md-6">
        <div class="card">
          <div class="card-header border-0"></div>
          <div class="card-body">
            <div class="m-md-5">
              <%= link_to "発注", "#", class: "btn btn-lg btn-block py-md-5" %>
            </div>
          </div>
        </div>
        <div class="card">
          <div class="card-header border-0"></div>
          <div class="card-body">
            <div class="m-md-5">
              <%= link_to "事業所", "#", class: "btn btn-lg btn-block py-md-5" %>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-6">  
        <div class="card">
          <div class="card-header border-0"></div>
          <div class="card-body">
            <div class="m-md-5">
              <%= link_to "作業員", "#", class: "btn btn-lg btn-block py-md-5" %>
            </div>
          </div>
        </div>
        <div class="card">
          <div class="card-header border-0"></div>
          <div class="card-body">
            <div class="m-md-5">
              <%= link_to "ユーザー", "#", class: "btn btn-lg btn-block py-md-5" %>
            </div>
